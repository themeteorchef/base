{
  "name": "application-name",
  "version": "1.0.0",
  "description": "Application description.",
  "scripts": {
    "start": "meteor --settings settings-development.json",
    "test": "meteor test --driver-package practicalmeteor:mocha --port 5000",
    "chimp-watch": "chimp --ddp=http://localhost:3000 --watch --mocha --path=tests",
    "chimp-test": "chimp --ddp=http://localhost:3000 --mocha --path=tests",
    "staging": "meteor deploy staging.meteor.com --settings settings-development.json",
    "production": "meteor deploy production.meteor.com --settings settings-production.json"
  },
  "devDependencies": {
    "chimp": "^0.34.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-meteor": "^3.5.2",
    "eslint-plugin-react": "^5.0.1",
    "faker": "^3.1.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "meteor",
      "react"
    ],
    "extends": [
      "airbnb/base",
      "plugin:meteor/guide",
      "plugin:react/recommended"
    ],
    "globals": {
      "$": false,
      "Accounts": false,
      "Bert": false,
      "browser": false,
      "expect": false,
      "Factory": false,
      "Meteor": false,
      "Roles": false,
      "server": false,
      "SimpleSchema": false,
      "ValidatedMethod": false
    },
    "rules": {}
  },
  "dependencies": {
    "apollo-client": "^0.3.9",
    "bootstrap": "^3.3.6",
    "express": "^4.13.4",
    "faker": "^3.1.0",
    "graphql": "^0.5.0",
    "graphql-tools": "^0.4.0",
    "http-proxy-middleware": "^0.15.0",
    "jquery": "^2.2.3",
    "jquery-validation": "^1.15.0",
    "moment": "^2.13.0",
    "mysql": "^2.10.2",
    "react": "^15.0.2",
    "react-addons-pure-render-mixin": "^15.0.1",
    "react-apollo": "^0.3.4",
    "react-bootstrap": "^0.29.3",
    "react-datepicker": "^0.27.0",
    "react-dom": "^15.0.2",
    "react-komposer": "^1.8.0",
    "react-router": "^2.4.0",
    "react-router-bootstrap": "^0.23.0",
    "redux": "^3.5.2",
    "sequelize": "^3.23.2"
  }
}
